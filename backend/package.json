{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/bin/www.js\"",
    "start": "node ./dist/bin/www.js",
    "startts":"node ./bin/www.ts",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"**/*.{ts,tsx}\"",
    "tslint": "tslint --project tsconfig.json"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.18.2",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.8",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.1",
    "concurrently": "^8.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.3"
  }
}
